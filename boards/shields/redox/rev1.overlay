/ {
    leds {
        compatible = "gpio-leds";

        led_caps: led_caps {
            gpios = <&gpio0 17 GPIO_ACTIVE_HIGH>;
            label = "led_caps";
        };

        led_lower: led_lower {
            gpios = <&gpio0 20 GPIO_ACTIVE_HIGH>;
            label = "led_lower";
        };

        led_raise: led_raise {
            gpios = <&gpio0 24 GPIO_ACTIVE_HIGH>;
            label = "led_raise";
        };
    };
};
